#include<stdio.h>
#include<conio.h>
int main()
{
	int arr[]={20,6,101,8,59,11,13,10,34,45};
	int len=sizeof(arr)/sizeof(int);
	int flag,i,n,sum=0,f=1;
	for(i=0;i<len;i++){
		n=arr[i];
		flag = 0;
		{
			if(flag==0){
				printf("%d\n",n);
				sum +=n;
			}
			printf("sum=%d\n",sum);
		}
		f=f*sum;
		sum= +sum;
	}
	printf("\nSum of Factorial = %d\n",f);
}
